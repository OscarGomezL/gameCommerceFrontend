{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/index.jsx", "pages/ds.jsx", "pages/cart.jsx", "pages/about.jsx", "components/Nav/myAccount.jsx", "components/cart/cartObject.jsx", "pages/wii.jsx", "pages/directions.jsx", "pages/xbox360.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
